<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="f1_iva_wizard_view" model="ir.ui.view">
            <field name="name">f1.iva.form</field>
            <field name="model">f1.iva</field>
            <field name="arch" type="xml">
                <form string="Generar F1 IVA">
                    <group>
                        <field name="fecha_inicio"/>
                        <field name="fecha_fin"/>
                    </group>
                    <footer>
                        <button name="generar_reporte" string="Generar" type="object" class="btn-primary"/>
                        <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>