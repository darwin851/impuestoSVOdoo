<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="libro_iva_compras_wizard_view" model="ir.ui.view">
            <field name="name">libro.iva.compras.form</field>
            <field name="model">libro.iva.compras</field>
            <field name="arch" type="xml">
                <form string="Generar Libro de IVA Compras">
                    <group>
                        <field name="fecha_inicio"/>
                        <field name="fecha_fin"/>
                    </group>
                    <footer>
                        <button name="generar_reporte" string="Generar" type="object" class="btn-primary"/>
                        <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>